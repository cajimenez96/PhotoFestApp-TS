{
  "cli": {
    "version": ">= 5.9.3"
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk"
      }
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "preview3": {
      "developmentClient": true
    },
    "preview4": {
      "distribution": "internal"
    },
    "production": {
      "android": {
        "environment": "production",
        "buildType": "app-bundle",
        "distribution": "store",
        "env": {
          "EXPO_UPDATES_CHANNEL": "production"
        }
      }
    }
  }
}